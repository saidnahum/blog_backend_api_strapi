{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "desc": {
      "type": "richtext",
      "required": true
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "collection": "category",
      "via": "posts",
      "dominant": true
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
